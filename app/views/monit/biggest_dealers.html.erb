<div class="row">
  <div class="col-md-12">
    <div class="panel panel-success">
      <div class="panel-heading">
        <h3 class="panel-title">
        <i class="fa fa-pie-chart"></i>
        <%= t("chart.biggest_dealers") %>
        </h3>
      </div>
      <div class="panel-body">
        <table class="table table-hover">
          <thead>
            <tr>
              <th>#</th>
              <th>Nome</th>
              <th>Tipo pgto</th>
              <th>Origem</th>
              <th>Valor</th>
            </tr>
          </thead>
          <tbody>
            <% (1..10).each do |row| %>
              <tr>
                <th scope="row"><%= row %></th>
                <td><%= ["Joefrey Nixxon", "Cidadão", "Chora bichão", "Amaral", "IPS", "Top Secret"].sample %></td>
                <td><%= ["A vista", "Parcelado"].sample %></td>
                <td><%= ["SEFIN", "PGM"].sample %></td>
                <td><%= number_to_currency rand(1000..1000000) %></td>
              </tr>

            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>

